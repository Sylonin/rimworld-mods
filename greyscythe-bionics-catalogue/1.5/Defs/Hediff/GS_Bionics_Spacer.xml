<!-- 
		*** handcannon_transform ***
-->
	<HediffDef ParentName="AddedBodyPartBase">
		<defName>GS_hediff_handcannon_transform</defName>
		<label>transformable hand-cannon</label>
		<labelNoun>an installed transformable hand-cannon</labelNoun>
		<defaultLabelColor>(0.5, 0.7, 1.0)</defaultLabelColor>
		<description>An installed transformable hand-cannon, a deceptively powerful short-barreled gun folded into a bionic arm, installed in such a way that it does not intefere with the performance of the arm.</description>
		<descriptionHyperlinks><ThingDef>GS_handcannon_transform</ThingDef></descriptionHyperlinks>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
				<li>
					<label>barrel</label>
					<capacities>
					<li>Blunt</li>
					</capacities>
					<power>10</power>
					<cooldownTime>2</cooldownTime>
				</li>
				</tools>
			<verbs>
			<li>
				<verbClass>Verb_Shoot</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>GS_Handcannon_Bullet_Spacer</defaultProjectile>
				<ticksBetweenBurstShots>35</ticksBetweenBurstShots>
				<burstShotCount>6</burstShotCount>
				<warmupTime>2.6</warmupTime>
				<range>18</range>
				<soundCast>Shot_Revolver</soundCast>
				<soundCastTail>GunTail_Light</soundCastTail>
				<muzzleFlashScale>9</muzzleFlashScale>
				<accuracyTouch>0.80</accuracyTouch>
				<accuracyShort>0.75</accuracyShort>
				<accuracyMedium>0.55</accuracyMedium>
				<accuracyLong>0.45</accuracyLong>
			</li>
			</verbs>
			</li>
		</comps>
		<spawnThingOnRemoved>GS_handcannon_transform</spawnThingOnRemoved>
		<addedPartProps>
		<solid>true</solid>
		<partEfficiency>1.25</partEfficiency>
		</addedPartProps>
	</HediffDef>
  
	<ThingDef ParentName="BaseBullet">
		<defName>GS_Handcannon_Bullet_Spacer</defName>
		<label>transformable hand-cannon bullet</label>
		<graphicData>
		<texPath>Things/Projectile/Bullet_Small</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile>
		<damageDef>Bullet</damageDef>
		<damageAmountBase>20</damageAmountBase>
		<stoppingPower>2</stoppingPower>
		<speed>70</speed>
		</projectile>
	</ThingDef>

	<ThingDef ParentName="GS_SpacerBionicMakeableBase">
		<defName>GS_handcannon_transform</defName>
		<label>transformable hand-cannon</label>
		<description>A combat-oriented hand replacement consisting of a transformable hand-cannon, a deceptively powerful short-barreled gun folded into a bionic arm, installed in such a way that it does not intefere with the performance of the arm.</description>
		<descriptionHyperlinks><RecipeDef>GS_Installhandcannon_transform</RecipeDef></descriptionHyperlinks>
		<costList>
		<Plasteel>30</Plasteel>
		<Steel>10</Steel>
		<ComponentSpacer>5</ComponentSpacer>
		</costList>
		<statBases>
		<Mass>5.8</Mass>
		</statBases>
		<techHediffsTags>
		<li>Advanced</li>
		</techHediffsTags>
	</ThingDef>

	<RecipeDef ParentName="GS_SurgeryInstallSpacerBionicBase">
		<defName>GS_Installhandcannon_transform</defName>
		<label>install transformable hand-cannon</label>
		<description>Install a transformable hand-cannon.</description>
		<descriptionHyperlinks>
		<ThingDef>GS_handcannon_transform</ThingDef>
		<HediffDef>GS_hediff_handcannon_transform</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing transformable hand-cannon.</jobString>
		<ingredients>
		<li>
			<filter>
			<thingDefs>
				<li>GS_handcannon_transform</li>
			</thingDefs>
			</filter>
			<count>1</count>
		</li>
		</ingredients>
		<fixedIngredientFilter>
		<thingDefs>
			<li>GS_handcannon_transform</li>
		</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
		<li>Shoulder</li>
		</appliedOnFixedBodyParts>
		<addsHediff>GS_hediff_handcannon_transform</addsHediff>
	</RecipeDef>

<!-- 
		*** machinegunarm_transform ***
-->
	<HediffDef ParentName="AddedBodyPartBase">
		<defName>GS_hediff_machinegunarm_transform</defName>
		<label>transformable machinegun-arm</label>
		<labelNoun>an installed transformable machinegun-arm</labelNoun>
		<defaultLabelColor>(0.5, 0.7, 1.0)</defaultLabelColor>
		<description>An installed transformable machinegun-arm, a powerful box-fed automatic weapon designed to fold into an advanced bionic arm. It still slows the user down slightly due to its weight, but performs perfectly fine as a bionic arm.</description>
		<descriptionHyperlinks><ThingDef>GS_machinegunarm_transform</ThingDef></descriptionHyperlinks>
		<stages>
			<li>
			<capMods>
				<li>
				<capacity>Moving</capacity>
				<offset>-0.1</offset>
				</li>
			</capMods>
			</li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
				<li>
					<label>fist</label>
					<capacities>
					<li>Blunt</li>
					</capacities>
					<power>8.2</power> <!-- Same as natural fist -->
					<cooldownTime>2</cooldownTime>
				</li>
				</tools>
			<verbs>
			<li>
				<verbClass>Verb_Shoot</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>GS_machinegunarm_transform_Bullet</defaultProjectile>
				<warmupTime>3.2</warmupTime>
				<range>32</range>
				<ticksBetweenBurstShots>4</ticksBetweenBurstShots>
				<burstShotCount>6</burstShotCount>
				<soundCast>Shot_Minigun</soundCast>
				<soundCastTail>GunTail_Medium</soundCastTail>
				<muzzleFlashScale>11</muzzleFlashScale>
				<accuracyTouch>0.42</accuracyTouch>
				<accuracyShort>0.55</accuracyShort>
				<accuracyMedium>0.45</accuracyMedium>
				<accuracyLong>0.32</accuracyLong>
			</li>
			</verbs>
			</li>
		</comps>
		<spawnThingOnRemoved>GS_machinegunarm_transform</spawnThingOnRemoved>
		<addedPartProps>
		<solid>true</solid>
		<partEfficiency>1.25</partEfficiency>
		</addedPartProps>
	</HediffDef>
  
	<ThingDef ParentName="BaseBullet">
		<defName>GS_machinegunarm_transform_Bullet</defName>
		<label>transformable machinegun-arm bullet</label>
