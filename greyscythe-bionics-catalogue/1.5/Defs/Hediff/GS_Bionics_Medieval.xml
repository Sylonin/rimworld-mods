				<warmupTime>3</warmupTime>
				<range>18</range>
				<soundCast>Bow_Recurve</soundCast>
			</li>
			</verbs>
			</li>
		</comps>
		<spawnThingOnRemoved>GS_handbow</spawnThingOnRemoved>
		<addedPartProps>
		<solid>true</solid>
		<partEfficiency>0.30</partEfficiency>
		</addedPartProps>
	</HediffDef>
  
